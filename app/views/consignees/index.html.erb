<div class="container">
<% if notice.present? %>
  <div class="alert alert-info" role="alert">
    <p id="notice"><%= notice %></p>
  </div>
<% end %>
<div class="panel panel-primary">

<div class="panel-heading">Listing Consignees</div>

<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Address line 1</th>
      <th>Po box no</th>
      <th>Tel no 1</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consignees.each do |consignee| %>
      <tr>
        <td><%= consignee.id %></td>
        <td><%= consignee.name %></td>
        <td><%= consignee.address_line_1 %></td>
        <td><%= consignee.po_box_no %></td>
        <td><%= consignee.tel_no_1 %></td>
        <td><%= consignee.email %></td>
        <td><%= link_to 'Show', consignee, class: "btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_consignee_path(consignee), class: "btn btn-warning" %></td>
        <td><%= link_to 'Destroy', consignee, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
     
    <% end %>
  </tbody>
</table>

</div>
<div class="digg_pagination">
    <center>   <%= will_paginate @consignees, renderer: BootstrapPagination::Rails %> </center>
  </div>
  <br>
<br>

<%= link_to 'New Consignee', new_consignee_path, class: "btn btn-primary" %>
</div>