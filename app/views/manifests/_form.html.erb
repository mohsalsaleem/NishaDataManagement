<%= simple_form_for(@manifest) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :mawb %>
    <%= f.association :order, as: :check_boxes, :label_method => lambda { |order| "#{order.hawb_number}" }%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
